<!DOCTYPE html>
<html lang="en">
  <head>
    <title>History</title>
    <link rel="stylesheet" type="text/css" href="/abc/css/travel_agency.css">
    <link rel="stylesheet" type="text/css" href="/abc/css/registration.css">
    <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<body class="app sidebar-mini">
  
  <% include ../travel_agency/header.ejs %>

  <% include ../travel_agency/sidenav.ejs %>
  
    <main class="app-content">

      <div class="app-title">
        <div>
          <h1>History</h1>
        </div>
      </div>

      <% for(var i=0 ; i < event.length; i++ ) { %>

      <div class="alert alert-info"> 
        <strong><a href="/events/event_details/<%=event[i].id%>">"<%=event[i].title%>"</a></strong>&nbsp event post is approved by admin.
      </div>
      <% } %>

      <% for(var i=0 ; i < data.length; i++ ) { %>

      <div class="alert alert-danger">
        <strong>"<%=data[i].title%>"</strong>&nbsp event post is declined/deleted.
      </div>
      <% } %>

    </main>
    <script src="/abc/js/jquery-3.3.1.min.js"></script>
    <script src="/abc/js/popper.min.js"></script>
    <script src="/abc/js/bootstrap.min.js"></script>
    <script src="/abc/js/main.js"></script>
  </body>
</html>